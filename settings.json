{
  "api_keys": {
    "deepgram": "YOUR_DEEPGRAM_API_KEY",
    "openai": "YOUR_OPENAI_API_KEY"
  },
  "api_urls": {
    "openai_base_url": "https://api.openai.com/v1"
  },
  "livekit": {
    "url": "YOUR_LIVEKIT_URL",
    "api_key": "YOUR_LIVEKIT_API_KEY",
    "api_secret": "YOUR_LIVEKIT_API_SECRET",
    "ws_url": "YOUR_LIVEKIT_WS_URL"
  },
  "agent": {
    "instructions": "You are a helpful voice AI assistant.",
    "greeting": "Greet the user and offer your assistance.",
    "stt": {
      "provider": "deepgram",
      "model": "nova-3",
      "language": "multi"
    },
    "llm": {
      "provider": "openai",
      "model": "gpt-4o-mini"
    },
    "tts": {
      "provider": "deepgram"
    },
    "vad": {
      "provider": "silero"
    },
    "turn_detection": {
      "provider": "english"
    },
    "noise_cancellation": {
      "provider": "bvc"
    }
  },
  "client": {
    "title": "LiveKit Voice Chat",
    "auto_connect": true
  },
  "deployment": {
    "backend": {
      "port": 8000
    },
    "client": {
      "port": 5173
    }
  }
}
